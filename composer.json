{
    "name": "php-di/php-di",
    "type": "library",
    "description": "The dependency injection container for humans",
    "keywords": ["di", "dependency injection", "container", "ioc", "psr-11", "psr11", "container-interop"],
    "homepage": "http://php-di.org/",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "DI\\": "src/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "DI\\Test\\IntegrationTest\\": "tests/IntegrationTest/",
            "DI\\Test\\UnitTest\\": "tests/UnitTest/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "format-code": "php-cs-fixer fix --allow-risky=yes",
        "phpstan": "phpstan analyse -l 6 -c phpstan.neon src"
    },
    "require": {
        "php": ">=7.0.0",
        "psr/container": "^1.0",
        "php-di/invoker": "^2.0",
        "php-di/phpdoc-reader": "^2.0.1",
        "jeremeamia/superclosure": "^2.0",
        "nikic/php-parser": "^2.0|^3.0|^4.0"
    },
    "require-dev": {
        "sebastian/diff": "^2.0|^3.0",
        "phpunit/phpunit-mock-objects": "~4.0|~5.0|~6.0",
        "phpunit/phpunit": "~6.4|~7.0",
        "mnapoli/phpunit-easymock": "~1.0",
        "doctrine/annotations": "~1.2",
        "ocramius/package-versions": "^1.2.0",
        "ocramius/proxy-manager": "^2.0.4",
        "friendsofphp/php-cs-fixer": "^2.4",
        "phpstan/phpstan": "^0.9.2|^0.10.0"
    },
    "provide": {
        "psr/container-implementation": "^1.0"
    },
    "suggest": {
        "doctrine/annotations": "Install it if you want to use annotations (version ~1.2)",
        "ocramius/proxy-manager": "Install it if you want to use lazy injection (version ~2.0)"
    }
}
